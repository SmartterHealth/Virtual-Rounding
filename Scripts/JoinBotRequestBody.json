    "meetingInfo": {
      "@odata.type": "#microsoft.graph.organizerMeetingInfo",
      "organizer": {
        "@odata.type": "#microsoft.graph.identitySet",
        "user": {
          "@odata.type": "#microsoft.graph.identity",
          "id": "{{organizerId}}",
